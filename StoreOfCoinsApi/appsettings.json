{
  "StoreOfCoinsDatabase": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "StoreOfCoins",
    "CoinsCollectionName": "Coins"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "Topics": {
      "ObjectsConfirmRequests": "objects.confirm.requests",
      "ObjectsConfirmResponses": "objects.confirm.responses"
    },
    "GroupIds": {
      "ObjectsConsumer": "storeofcoins-objects-consumer",
      "UsersConsumer": "storeofcoins-users-consumer"
    }
  }
}